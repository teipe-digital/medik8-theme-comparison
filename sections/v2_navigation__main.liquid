<global-header-main class="flex column-nowrap">
  <div class="block-grid--index block-c mh__container">
    <div class="mh__wrapper">
      <div class="mh__top-bar @tablet__hide">
        <div class="flex block-r gbl-justify-right">
          {%- assign header_extra_links = section.settings.header_extra_links.links -%}
          <ul class="links-list">
            {%- if header_extra_links.size > 0 -%}
              {%- for link in header_extra_links -%}
                <li class="links-list__item">
                  {%- if link.title contains "Hiring" -%}
                    <a href="{{ link.url }}" class="links-list__link">
                      {{ link.title }}
                      {%- render 'icon.v2_nav.red-dot.svg' -%}
                    </a>
                  {%- elsif link.title contains "Reward" -%}
                    {%- liquid 
                      assign hasDropDown = false
                      for block in section.blocks
                        if block.settings.title == link.title 
                          assign hasDropDown = block
                          break
                        endif
                      endfor
                    -%}
                    <div class="mh__main-link-wrap flex{% if hasDropDown %} mh__main-link-wrap--has-dropdown has-dropdown{% endif %}">
                      <a href="{{ link.url }}" class="links-list__link{% if hasDropDown %} has-underline-effect has-underline-effect--full-width{% endif %}">
                        {%- render 'icon.v2_nav.crystal.svg' -%}
                        {{ link.title }}
                      </a>
                      {% if hasDropDown %}
                        {% render 'v2_navigation.header.dropdown', block:hasDropDown %}
                      {% endif %}
                    </div>
                  {%- else -%}
                    <a href="{{ link.url }}" class="links-list__link">
                      {{ link.title }}
                    </a>
                  {% endif %}
                </li>
              {%- endfor -%}
            {%- endif -%}
            <li class="links-list__item mh__item--country">
              {%- if settings.country_selector_enabled -%}
                {%- render 'country-selector', show_currency: false, class: " flex" -%}
              {%- endif -%}
            </li>
          </ul>
        </div>
      </div>

      <a aria-label="Home" class="mh__logo" href="{{ shop.secure_url }}" itemprop="url">

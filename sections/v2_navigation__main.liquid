      <nav class="mh__main-nav flex row-wrap align-center justify-center @tablet__hide">
        {% for link in section.settings.main-menu.links %}
          {% liquid 
            assign hasDropDown = false
            for block in section.blocks
              if block.settings.title == link.title 
                assign hasDropDown = block
                break
              endif
            endfor
          %}
          <div class="mh__main-link-wrap flex{% if hasDropDown %} mh__main-link-wrap--has-dropdown has-dropdown{% endif %}">
            <a class="mh__main-link t-rm f-w400{% if hasDropDown %} has-underline-effect{% endif %}" href="{{ link.url }}">{{ link.title }}</a>
            {% if hasDropDown %}
              {% render 'v2_navigation.header.dropdown', block:hasDropDown %}
            {% endif %}
          </div>
        {% endfor %}
      </nav>

      <div class="flex row-wrap align-center justify-right mh__header-buttons flex-no-wrap">
        <button aria-label="Search" id="header-search-icon" class="mh__icon-nav-item">
          {%- render 'icon.v2_nav.search.svg' -%}
        </button>
        {% comment %} {%- if settings.country_selector_enabled -%}
          {%- render 'country-selector', class: " mh__icon-nav-item mh__icon-nav-item--country", show_full_name: false -%}
        {%- endif -%} {% endcomment %}
        <div class="@tablet__hide">
          <a aria-label="Wishlist" href="#swym-wishlist" class="swym-wishlist mh__icon-nav-item">
            {%- render 'icon.v2_nav.wishlist.svg' -%}
          </a>
        </div>
        <div style="display:none;" id="swym-wishlist">
        </div>
        {% if shop.customer_accounts_enabled %}
        <a 
          aria-label="My account" 
          class="mh__icon-nav-item"
          href="{%- if customer -%}{{ routes.account_url }}{%- else -%}{{ routes.account_login_url }}{%- endif -%}" class="mh__icon-nav-item flex @tablet__hide">
          {%- render 'icon.v2_nav.account.svg' -%}
        </a>
        {% endif %}
        {% comment %} {% if section.settings.account_sibling_link_text != blank and section.settings.account_sibling_link_url != blank %}
          {% liquid 
            assign hasDropDown = false
            for block in section.blocks
              if block.settings.title == section.settings.account_sibling_link_text
                assign hasDropDown = block
                break
              endif
            endfor
          %}
          <div class="mh__main-link-wrap mh__main-link-wrap--rewards{% if hasDropDown %} mh__main-link-wrap--has-dropdown has-dropdown{% endif %}">
            <a class="mh__main-link t-s f-w400" href="{{ section.settings.account_sibling_link_url }}">{{ section.settings.account_sibling_link_text }}</a>
            {% if hasDropDown %}
              {% render 'v2_navigation.header.dropdown', block:hasDropDown %}
            {% endif %}
          </div>
        {% endif %} {% endcomment %}
        <a 
          href="/cart" 
          class=" js-cart-toggle mh__icon-nav-item mh__icon-nav-item--bag block-rel"
          aria-controls="CartDrawer"
          aria-expanded="false">
          {% render 'icon.v2_nav.bag.svg' %}
          <span class="js-global-cart-counter mh__cart-counter">{{ cart.item_count }}</span>
        </a>
        <button aria-label="Mobile menu" id="navMenuTrigger" class="js-sidebar-mobile__toggle mh__icon-nav-item mh__icon-nav-item--mob-menu btn-reset @tablet__show burger" aria-expanded="false">
          <div class="burger__container">
            <span class="burger__line burger__line--1"></span>
            <span class="burger__line burger__line--2"></span>
            <span class="burger__line burger__line--3"></span>
          </div>
        </button>
      </div>
    </div>
  </div>
</global-header-main>

<script type="text/javascript">
  DomReadyPromise().then( () => {

    // create an observer to pass the dropdowns data to mobile nav
    const HeaderMainSectionDataProducer$ = new ObserverLite({key:'HeaderMainSectionDataProducer$'})
    HeaderMainSectionDataProducer$.next({
      settings: {{ section.settings | json }},
      blocks: [
        {% for block in section.blocks %}
          {
            type: {{ block.type | json }},
            settings: {{ block.settings | json }},
            links:[
              {% for link in block.settings.linklist.links %}
                {
                  url: {{ link.url | json }},
                  title: {{ link.title | json }},
                  links:[
                    {% for link in link.links %}
                      {
                        url: {{ link.url | json }},
                        title: {{ link.title | json }}
                      }
                      {% unless forloop.last %},{% endunless %}
                    {% endfor %}
                  ]
                }
                {% unless forloop.last %},{% endunless %}
              {% endfor %}
            ]
          }
          {% unless forloop.last %},{% endunless %}
        {% endfor %}
      ]
    })
  
    
    // init countries data util
    window.countriesData = new CountriesData({
      jsonDataUrl:{{ 'countries.json' | asset_url | json }},
      selectorSettings:{
        country_selector_heading:{{ settings.country_selector_heading | json }},
        country_selector_footer_url: {{ settings.country_selector_footer_url | json }},
        country_selector_footer_text: {{ settings.country_selector_footer_text | json }}
      }
    })
    
    {%- render 'v2-global__geo-locator-popup.js' -%}
  })
</script>

